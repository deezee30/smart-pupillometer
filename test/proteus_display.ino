void setup() {
    Serial.begin(115200);
    while (!Serial)
        ;
    Serial.println();

    // settling time
    delay(250);

    // Set up TFT display
    tft.init();
    tft.setRotation(-1);
    tft.fillScreen(TFT_BLACK);
    tft.setTextColor(TFT_WHITE, TFT_BLACK);
    tft.setTextSize(1);

    // Draw heading: Title
    tft.fillRect(0, 0, getWidth(), 16, TFT_LIGHTGREY);
    tft.setCursor(25, 0, 2);
    tft.print("Smart Pupillometer");

    // Draw left sidebar: Options
    tft.fillRect(0, 16, 24, getHeight() - 16, TFT_DARKGREY);

    // Draw right sidebar: Scale
    tft.fillRect(getWidth() - 24, 16, 24, getHeight() - 16, TFT_DARKGREY);

    // Fill pixels
    for (uint8_t r = 0; r < 48; r++) {
        for (uint8_t c = 0; c < 48; c++) {
            tft.drawPixel(r, c, image[r][c]);
        }
    }
}

